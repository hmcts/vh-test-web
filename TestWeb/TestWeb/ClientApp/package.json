{
  "name": "testweb",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "build:ssr": "ng run TestWeb:server:dev",
    "test": "ng test --code-coverage",
    "test-once": "ng test --watch=false --code-coverage",
    "test-once-ci": "ng test --browsers=ChromeHeadlessNoPrompt --watch=false --code-coverage",
    "lint": "ng lint && npm run prettify",
    "prettify": "run-script-os",
    "prettify:nix": "npx prettier -c 'src/app/**/*.*'",
    "prettify:default": "npx prettier -c \"src\\app\\**\\*.*\"",
    "prettify-fix": "run-script-os",
    "prettify-fix:nix": "npx prettier --write 'src/app/**/*.*'",
    "prettify-fix:default": "npx prettier --write \"src\\app\\**\\*.*\"",
    "snyk-protect": "snyk protect",
    "prettify-api-client": "run-script-os",
    "prettify-api-client:nix": "npx prettier --write 'src/app/services/clients/api-client.ts'",
    "prettify-api-client:default": "npx prettier --write \"src\\app\\services\\clients\\api-client.ts\"",
    "update-api-client": "nswag run && npm run prettify-api-client"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.1.0",
    "@angular/common": "11.1.0",
    "@angular/compiler": "11.1.0",
    "@angular/core": "11.1.0",
    "@angular/forms": "11.1.0",
    "@angular/localize": "^11.1.0",
    "@angular/platform-browser": "11.1.0",
    "@angular/platform-browser-dynamic": "11.1.0",
    "@angular/platform-server": "11.1.0",
    "@angular/router": "11.1.0",
    "@hmcts/frontend": "^0.0.41-alpha",
    "@microsoft/applicationinsights-web": "^2.5.11",
    "@nguniversal/module-map-ngfactory-loader": "8.2.6",
    "adal-angular4": "^3.0.16",
    "aspnet-prerendering": "^3.0.1",
    "bootstrap": "^4.6.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.8.3",
    "govuk-frontend": "^3.10.2",
    "guid-typescript": "^1.0.9",
    "ngx-clipboard": "^13.0.1",
    "ngx-spinner": "^10.0.1",
    "oidc-client": "^1.9.1",
    "popper.js": "^1.16.0",
    "protractor": "~7.0.0",
    "run-script-os": "^1.1.5",
    "rxjs": "^6.6.3",
    "sass": "^1.32.5",
    "snyk": "^1.439.1",
    "ts-node": "~8.4.1",
    "tslib": "^2.1.0",
    "tslint": "~6.1.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.7",
    "@angular/cli": "^11.1.1",
    "@angular/compiler-cli": "^11.1.0",
    "@angular/language-service": "^11.1.0",
    "@types/jasmine": "^3.6.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.19.15",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^2.0.1",
    "ng-bullet": "^1.0.3",
    "nswag": "^13.10.1",
    "prettier": "^2.2.1",
    "puppeteer": "^5.5.0",
    "typescript": "~4.0.3"
  },
  "optionalDependencies": {
    "protractor": "~7.0.0",
    "ts-node": "~8.4.1",
    "tslint": "~6.1.3"
  }
}
